<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="ir_cron_get_refresh_token_lzd" model="ir.cron">
            <field name='name'>Refresh Token Lazada</field>
            <field name="model_id" ref="base.model_ir_config_parameter" />
            <field name="interval_number">6</field>
            <field name='interval_type'>days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True" />
            <field name="doall" eval="False" />
            <field name="code">model.action_lazada_refresh_token()</field>
            <field name="state">code</field>
        </record>

    </data>
</odoo>